{
    "test": {
        "driver": "sqlite3",
        "database": ":memory:",
        "filename": "./dev_shopping.db"
    },
    "dev": {
        "driver": "pg",
        "host": {
            "ENV": "PGHOST"
        },
        "port": {
            "ENV": "PGPORT"
        },
        "user": {
            "ENV": "PGUSER"
        },
        "password": {
            "ENV": "PGPASSWORD"
        },
        "dbname": {
            "ENV": "PGDATABASE"
        },
        "database": {
            "ENV": "PGDATABASE"
        },
        "sslmode": "require",
        "schema": "shopping"
    }
}